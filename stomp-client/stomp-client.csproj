<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>NikitaTrubkin.StompClient</RootNamespace>
    <Authors>Nikita Trubkin</Authors>
    <Description>Stomp client over websockets</Description>
    <PackageLicenseFile></PackageLicenseFile>
    <RepositoryUrl>https://github.com/NTrubkin/stomp-csharp-client</RepositoryUrl>
    <AssemblyVersion>0.1.0.0</AssemblyVersion>
    <FileVersion>0.1.0.0</FileVersion>
    <Version>0.1</Version>
    <RepositoryType />
    <PackageTags>stomp, websocket</PackageTags>
    <PackageReleaseNotes>Works with Spring server (StompEndpointRegistry.withSockJS() configuration)</PackageReleaseNotes>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Product>StompClient</Product>
    <PackageId>StompClient</PackageId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
    <PackageReference Include="WebSocketSharp-NonPreRelease" Version="1.0.0" />
  </ItemGroup>

</Project>
